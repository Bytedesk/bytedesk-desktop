  "scripts": {
    "dev": "vite --host --mode dev",
    "start": "vite --host --mode dev",
    "build": "tsc && vite build --mode prod && electron-builder",
    "build:web": "tsc && vite build --mode web",
    "build:open": "tsc && vite build --mode open",
    "build:mac:mas": "tsc && vite build --mode prod && electron-builder --mac --config electron-builder-mac-mas.yml",
    "build:mac:dmg": "tsc && vite build --mode prod && electron-builder --mac --config electron-builder-mac-dmg.yml",
    "build:win": "tsc && vite build --mode prod && electron-builder --win --config electron-builder-windows.yml",
    "build:linux": "tsc && vite build --mode prod && electron-builder --linux --config electron-builder-linux.yml",
    "release:web": "sh cicd/scripts/build-upload.sh",
    "release:open": "sh cicd/scripts/build-open.sh",
    "release:mac": "sh cicd/scripts/build-upload-mac.sh",
    "release:mas": "sh cicd/scripts/build-mac-mas.sh",
    "release:win": "sh cicd/scripts/build-upload-win.sh",
    "release:linux": "sh cicd/scripts/build-upload-linux.sh",
    "preview": "vite preview --host",
    "pree2e": "vite build --mode=test",
    "e2e": "playwright test"
  },
